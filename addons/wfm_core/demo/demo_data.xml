            <field name="is_wfm_client">True</field>
            <field name="street">Λεωφ. Συγγρού 350</field>
            <field name="city">Καλλιθέα</field>
            <field name="postal_code">17674</field>
            <field name="phone">+30 210 1234567</field>
            <field name="email">info@alpha-industry.gr</field>
        </record>

        <record id="demo_client_beta" model="res.partner">
            <field name="name">ΒΗΤΑ ΕΜΠΟΡΙΚΗ Ε.Π.Ε.</field>
            <field name="is_company">True</field>
            <field name="is_wfm_client">True</field>
            <field name="street">Μιχαλακοπούλου 48</field>
            <field name="city">Αθήνα</field>
            <field name="postal_code">11528</field>
            <field name="phone">+30 210 2345678</field>
            <field name="email">info@beta-commerce.gr</field>
        </record>

        <record id="demo_client_gamma" model="res.partner">
            <field name="name">ΓΑΜΜΑ ΚΑΤΑΣΚΕΥΑΣΤΙΚΗ Α.Ε.</field>
            <field name="is_company">True</field>
            <field name="is_wfm_client">True</field>
            <field name="street">Κηφισίας 120</field>
            <field name="city">Αθήνα</field>
            <field name="postal_code">11526</field>
            <field name="phone">+30 210 3456789</field>
            <field name="email">info@gamma-construction.gr</field>
        </record>

        <!-- Demo Partners -->
        <record id="demo_partner_papadopoulos" model="res.partner">
            <field name="name">Δρ. Νίκος Παπαδόπουλος</field>
            <field name="is_wfm_partner">True</field>
            <field name="specialty">physician</field>
            <field name="hourly_rate">80.00</field>
            <field name="city">Αθήνα</field>
            <field name="phone">+30 694 1234567</field>
            <field name="email">n.papadopoulos@ohs-partners.gr</field>
        </record>

        <record id="demo_partner_konstantinou" model="res.partner">
            <field name="name">Μαρία Κωνσταντίνου</field>
            <field name="is_wfm_partner">True</field>
            <field name="specialty">safety_engineer</field>
            <field name="hourly_rate">65.00</field>
            <field name="city">Πειραιάς</field>
            <field name="phone">+30 694 2345678</field>
            <field name="email">m.konstantinou@ohs-partners.gr</field>
        </record>

        <record id="demo_partner_antoniou" model="res.partner">
            <field name="name">Δρ. Γιώργος Αντωνίου</field>
            <field name="is_wfm_partner">True</field>
            <field name="specialty">physician</field>
            <field name="hourly_rate">85.00</field>
            <field name="city">Θεσσαλονίκη</field>
            <field name="phone">+30 694 3456789</field>
            <field name="email">g.antoniou@ohs-partners.gr</field>
        </record>

        <!-- Demo Installations -->
        <record id="demo_installation_alpha_hq" model="wfm.installation">
            <field name="name">Κεντρικά Γραφεία</field>
            <field name="client_id" ref="demo_client_alpha"/>
            <field name="street">Λεωφ. Συγγρού 350</field>
            <field name="city">Καλλιθέα</field>
            <field name="postal_code">17674</field>
            <field name="installation_type">office</field>
            <field name="employee_count">120</field>
        </record>

        <record id="demo_installation_alpha_warehouse" model="wfm.installation">
            <field name="name">Αποθήκη Ασπροπύργου</field>
            <field name="client_id" ref="demo_client_alpha"/>
            <field name="street">Λεωφ. ΝΑΤΟ 45</field>
            <field name="city">Ασπρόπυργος</field>
            <field name="postal_code">19300</field>
            <field name="installation_type">warehouse</field>
            <field name="employee_count">45</field>
        </record>

        <record id="demo_installation_beta_store" model="wfm.installation">
            <field name="name">Κατάστημα Κηφισιάς</field>
            <field name="client_id" ref="demo_client_beta"/>
            <field name="street">Κηφισίας 200</field>
            <field name="city">Κηφισιά</field>
            <field name="postal_code">14562</field>
            <field name="installation_type">retail</field>
            <field name="employee_count">25</field>
        </record>

        <record id="demo_installation_gamma_site" model="wfm.installation">
            <field name="name">Εργοτάξιο Μαρούσι</field>
            <field name="client_id" ref="demo_client_gamma"/>
            <field name="street">Αμαρουσίου-Χαλανδρίου 50</field>
            <field name="city">Μαρούσι</field>
            <field name="postal_code">15125</field>
            <field name="installation_type">construction</field>
            <field name="employee_count">80</field>
        </record>

        <!-- Demo Visits -->
        <record id="demo_visit_1" model="wfm.visit">
            <field name="client_id" ref="demo_client_alpha"/>
            <field name="installation_id" ref="demo_installation_alpha_hq"/>
            <field name="partner_id" ref="demo_partner_papadopoulos"/>
            <field name="visit_date" eval="(DateTime.today() + relativedelta(days=2)).strftime('%Y-%m-%d')"/>
            <field name="start_time">9.0</field>
            <field name="end_time">13.0</field>
            <field name="state">assigned</field>
            <field name="stage_id" ref="stage_assigned"/>
            <field name="visit_type">regular</field>
        </record>

        <record id="demo_visit_2" model="wfm.visit">
            <field name="client_id" ref="demo_client_alpha"/>
            <field name="installation_id" ref="demo_installation_alpha_warehouse"/>
            <field name="partner_id" ref="demo_partner_konstantinou"/>
            <field name="visit_date" eval="(DateTime.today() + relativedelta(days=5)).strftime('%Y-%m-%d')"/>
            <field name="start_time">10.0</field>
            <field name="end_time">14.0</field>
            <field name="state">confirmed</field>
            <field name="stage_id" ref="stage_confirmed"/>
            <field name="visit_type">regular</field>
        </record>

        <record id="demo_visit_3" model="wfm.visit">
            <field name="client_id" ref="demo_client_beta"/>
            <field name="installation_id" ref="demo_installation_beta_store"/>
            <field name="visit_date" eval="(DateTime.today() + relativedelta(days=1)).strftime('%Y-%m-%d')"/>
            <field name="start_time">14.0</field>
            <field name="end_time">17.0</field>
            <field name="state">draft</field>
            <field name="stage_id" ref="stage_draft"/>
            <field name="visit_type">regular</field>
        </record>

        <record id="demo_visit_4" model="wfm.visit">
            <field name="client_id" ref="demo_client_gamma"/>
            <field name="installation_id" ref="demo_installation_gamma_site"/>
            <field name="partner_id" ref="demo_partner_antoniou"/>
            <field name="visit_date" eval="(DateTime.today() - relativedelta(days=1)).strftime('%Y-%m-%d')"/>
            <field name="start_time">8.0</field>
            <field name="end_time">12.0</field>
            <field name="state">done</field>
            <field name="stage_id" ref="stage_completed"/>
            <field name="visit_type">urgent</field>
        </record>

        <record id="demo_visit_5" model="wfm.visit">
            <field name="client_id" ref="demo_client_gamma"/>
            <field name="installation_id" ref="demo_installation_gamma_site"/>
            <field name="partner_id" ref="demo_partner_konstantinou"/>
            <field name="visit_date" eval="DateTime.today().strftime('%Y-%m-%d')"/>
            <field name="start_time">9.0</field>
            <field name="end_time">13.0</field>
            <field name="state">in_progress</field>
            <field name="stage_id" ref="stage_in_progress"/>
            <field name="visit_type">follow_up</field>
        </record>
    </data>
</odoo>
